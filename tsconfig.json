{
    //  "extends": "@nuxt/vue-app-edge",
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": [
            "esnext",
            "esnext.asynciterable",
            "dom"
        ],
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "allowJs": true,
        "sourceMap": true,
        "strict": false,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "noEmit": true,
        "baseUrl": ".",
        "resolveJsonModule": true,
        "paths": {
            "~/*": [
                "./*"
            ],
            "@/*": [
                "./*"
            ]
        },
        "types": [
            "@nuxt/vue-app",
            "@types/node"
        ]
    },
    "include": [
        "types/**/*",
        "plugins/*",
        "**/*.ts",
        "**/*.vue", "src/Calculator/OffsetCalcCssBuilderts"
    ],
    "exclude": [
        "node_modules"
    ]
}
